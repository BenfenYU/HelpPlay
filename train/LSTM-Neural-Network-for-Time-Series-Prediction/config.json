
{
	"data": {
		"filename": "one_dim_time.csv",
		"columns": [
			"wait_time"
		],
		"sequence_length": 31,
		"train_test_split": 0.85,
		"normalise": false 
	},
	"training": {
		"epochs": 60,
		"batch_size": 64
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 10,
				"input_timesteps": 30,
				"input_dim": 1,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "relu"
			}
		]
	}
}
